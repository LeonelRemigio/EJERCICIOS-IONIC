.config(function($stateProvider, $urlRouterProvider) {

$stateProvider
.state('home', { url: '',
templateUrl: 'templates/home.html', controller: 'homeCtrl'
})
.state('page', { url: '/page',
templateUrl: 'templates/page.html', controller: 'pageCtrl'
});

// PÃ¡gina por defecto
$urlRouterProvider.otherwise('');
})
